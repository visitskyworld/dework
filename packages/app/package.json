    "antd": "^4.17.1",
    "between": "^0.1.3",
    "graphql-tag": "^2.12.6",
    "lodash": "^4.17.21",
    "next": "12.0.4",
    "next-server": "^9.0.5",
    "next-with-apollo": "^5.2.1",
    "next-with-less": "^2.0.2",
    "nookies": "^2.5.2",
    "react": "17.0.2",
    "react-beautiful-dnd": "^13.1.0",
    "react-dom": "17.0.2"
  },
  "devDependencies": {
    "@types/lodash": "^4.14.177",
    "@types/node": "16.11.10",
    "@types/react": "^17.0.37",
    "@types/react-beautiful-dnd": "^13.1.2",
    "apollo": "^2.33.9",
    "env-cmd": "^10.1.0",
    "eslint": "7.32.0",
